# LLM Trademark AI Agent Environment Variables
# Copy this file to .env and fill in your actual values

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here

# RapidAPI Configuration (USPTO Trademark API)
RAPIDAPI_KEY=your_rapidapi_key_here
RAPIDAPI_HOST=uspto-trademark.p.rapidapi.com

# Environment Configuration
ENVIRONMENT=development
# Options: development, production

# LLM Model Configuration
LLM_MODEL=gpt-3.5-turbo
# Options: gpt-3.5-turbo, gpt-4, gpt-4-turbo-preview

# API Rate Limiting (Optional - for advanced users)
API_RATE_LIMIT=100
API_RATE_LIMIT_WINDOW=3600

# Logging Configuration (Optional)
LOG_LEVEL=INFO
# Options: DEBUG, INFO, WARNING, ERROR, CRITICAL

# Cache Configuration (Optional)
CACHE_ENABLED=true
CACHE_TTL=3600

# Search Configuration (Optional)
MAX_LLM_RESULTS=10

# Example values (DO NOT USE THESE IN PRODUCTION):
# OPENAI_API_KEY=sk-abc123def456ghi789
# RAPIDAPI_KEY=abc123def456ghi789
# ENVIRONMENT=production
